{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "QEMU i686 kernel (remote gdb)",
      "type": "cppdbg",
      "request": "launch",
      "MIMode": "gdb",
      "miDebuggerPath": "/home/jaideep/opt/cross/bin/i686-elf-gdb",
      "program": "${workspaceFolder}/build/i686/kernel.elf",
      "cwd": "${workspaceFolder}",
      "miDebuggerServerAddress": "localhost:1234",
      "stopAtConnect": true,
      "targetArchitecture": "x86",
      "setupCommands": [
        { "text": "set architecture i386" },
        { "text": "add-symbol-file ${workspaceFolder}/build/i686/kernel.elf 0xC0000000 -s .boot 0x00200000" }
      ],
    }
  ]
}
